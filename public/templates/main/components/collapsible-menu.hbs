<span {{action 'toggleMenu'}} {{bind-attr class=':title isCollapsed:collapsed'}}>
	{{#if showMenuIcon}}
	{{svg 'toc' viewBox='0 0 18 13' class='icon toc'}}
	{{/if}}
	{{svg 'chevron' viewBox='0 0 12 7' class='icon chevron'}}
	{{i18n tLabel count=tCount}}
</span>
{{#if ordered}}
<ol>
	{{yield}}
</ol>
{{else}}
<ul>
	{{yield}}
</ul>
{{/if}}


